<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiamu.publisher.mapper.UpdateSwitchMapper">
    <update id="updateStatus">
        UPDATE switch
        SET status = #{enabled}
        where switch='switch'
    </update>
</mapper>